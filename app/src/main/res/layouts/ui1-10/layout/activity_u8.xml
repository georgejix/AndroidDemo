<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.jx.androiddemo.view.GeneralTitleLayout
        android:id="@+id/layout_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.jx.androiddemo.testactivity.ui.ui1to10.ui8.MyLinearLayout
        android:id="@+id/layout"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:background="#00ff00"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <com.jx.androiddemo.testactivity.ui.ui1to10.ui8.MyLinearLayout2
            android:layout_width="368dp"
            android:layout_height="300dp"
            android:background="#ff0000"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <com.jx.androiddemo.testactivity.ui.ui1to10.ui8.MyTextView
                android:id="@+id/textview_test1"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_margin="50dp"
                android:background="#eee"
                android:gravity="center"
                android:text="test"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="0dp" />
        </com.jx.androiddemo.testactivity.ui.ui1to10.ui8.MyLinearLayout2>
    </com.jx.androiddemo.testactivity.ui.ui1to10.ui8.MyLinearLayout>

</LinearLayout>
